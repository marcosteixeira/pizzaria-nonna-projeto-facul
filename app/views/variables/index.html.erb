<h1>Listing variables</h1>

<table>
  <thead>
  <tr>
    <th><%= t("activerecord.attributes.variable.code") %></th>
    <th><%= t("activerecord.attributes.variable.exibition_name") %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @variables.each do |variable| %>
      <tr>
        <td><%= variable.code %></td>
        <td><%= variable.name %></td>
        <td><%= link_to 'Show', variable %></td>
        <td><%= link_to 'Edit', edit_variable_path(variable) %></td>
        <td><%= link_to 'Destroy', variable, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to t("helpers.submit.create", model: t("activerecord.models.variable")), new_variable_path %>
